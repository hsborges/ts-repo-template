{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "google",
    "prettier"
  ],
  "rules": {
    "valid-jsdoc": "off",
    "no-console": 2,
    "@typescript-eslint/no-explicit-any": 0
  }
}
